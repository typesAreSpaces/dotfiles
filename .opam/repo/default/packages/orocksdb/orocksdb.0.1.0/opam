  "conf-rocksdb"
  "ctypes" {>= "0.4.0"}
  "ctypes-foreign" {>= "0.4.0"}
  "depext" {build}
  "ocamlbuild" {build}
]
available: [ocaml-version >= "4.02.1" & opam-version >= "1.2"]
